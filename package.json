        "gatsby-plugin-typescript": "^2.4.21",
        "gatsby-remark-prismjs": "^3.5.16",
        "gatsby-source-filesystem": "^2.3.34",
        "gatsby-transformer-remark": "^2.8.38",
        "gatsby-transformer-yaml": "^2.4.14",
        "glamor": "^2.20.40",
        "prismjs": "^1.22.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.1.0",
        "react-icons": "^3.11.0",
        "react-use-flexsearch": "^0.1.1",
        "typescript": "^4.0.3",
        "yaml": "^1.10.0"
