    "devDependencies": {
        "@types/node": "^14.11.8",
        "@types/react": "^16.9.52",
        "@types/react-dom": "^16.9.8",
        "@types/react-helmet": "^6.1.0",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "eslint": "^7.11.0",
        "gh-pages": "^3.1.0"
